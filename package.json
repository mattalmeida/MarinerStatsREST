  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mattalmeida/MarinerStatsREST/issues"
  },
  "homepage": "https://github.com/mattalmeida/MarinerStatsREST#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "pg": "^8.2.2",
    "pg-promise": "^10.5.8"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.0.19",
    "@types/pg": "^7.14.3",
    "@types/shelljs": "^0.8.8",
    "fs-extra": "^9.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  }
